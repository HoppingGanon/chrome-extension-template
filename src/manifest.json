{
  "manifest_version": 3,
  "name": "Template",
  "version": "1.0.0",
  "icons": {
    "16": "assets/logo/16x16.png",
    "48": "assets/logo/48x48.png",
    "128": "assets/logo/128x128.png"
  },
  "description": "This is Template",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/onload.js"]
    }
  ],
  "action": {
    "default_popup": "popup/index.html"
  },
  "permissions": ["tabs", "storage"]
}
